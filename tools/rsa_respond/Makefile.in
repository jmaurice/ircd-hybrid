# $Id: Makefile.in,v 1.1 2003/06/20 01:43:35 joshk Exp $

CC = @CC@
LIBS = @LIBS@
CFLAGS = @CFLAGS@
INCLUDES = -I@INCLUDES@

all: respond

respond: respond.c
	$(CC) $(CFLAGS) -o respond respond.c ${INCLUDES} ${LDFLAGS} ${LIBS}

clean:
	rm -f respond *.core *~ *.exe

distclean: clean
	rm -f Makefile config.status config.log
	rm -rf autom4te.cache
